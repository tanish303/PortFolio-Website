generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GuestbookEntry {
  id        String   @id @default(cuid())
  name      String
  message   String
  createdAt DateTime @default(now())
}
